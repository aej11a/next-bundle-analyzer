    "@josselinbuils/exec-staged": "1.0.2",
    "@prefresh/webpack": "3.3.2",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.176",
    "@types/node": "16.11.7",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.3.1",
